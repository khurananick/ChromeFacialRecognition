{
  "manifest_version": 2,
  "name": "Web Face Recognition",
  "version": "0.0.1",
  "background": {
    "page": "/html/background.html"
  },
  "browser_action": {},
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["/js/libs/jquery-2.1.4.min.js","/js/content.js"],
    "run_at":"document_start"
  }],
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "management"
  ]
}

